{
  "name": "cviebrock/laravel-mangopay",
  "description": "Laravel/Lumen wrapper for the office Mangopay SDK library",
  "keywords": [
    "laravel",
    "lumen",
    "mangopay",
    "payments"
  ],
  "authors": [
    {
      "name": "Colin Viebrock",
      "email": "colin@viebrock.ca"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "illuminate/support": "^6.0",
    "mangopay/php-sdk-v2": "^2.4"
  },
  "require-dev": {
    "limedeck/phpunit-detailed-printer": "^5.0",
    "orchestra/testbench": "4.*",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Cviebrock\\LaravelMangopay\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cviebrock\\LaravelMangopay\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
      "laravel": {
          "providers": [
              "Cviebrock\\LaravelMangopay\\ServiceProvider"
          ]
      }
  }
}
